package Models;

/**Constructor used to create Country objects and generate their values. */
public class Country {

    /** Country ID */
    private int countryId;

    /** Contact name */
    private String country;

    /** Constructor for the Country object
     * @param countryId The Country ID Integer
     * @param country The Country name String
     * */
    public Country (int countryId, String country) {
        this.countryId = countryId;
        this.country = country;
    }

    /** Gets Country ID
     * @return countryID Integer for  Country ID
     * */
    public int getCountryId() {
        return countryId;
    }

    /** Gets Country
     * @return String for Country name
     * */
    public String getCountry() {
        return country;
    }

    /** Sets Country ID
     * @param countryId Integer for Country ID
     * */
    public void setCountryId(int countryId) {
        this.countryId = countryId;
    }

    /** Sets Country
     * @param country String for Country name
     * */
    public void setCountry(String country) {
        this.country = country;
    }
}
